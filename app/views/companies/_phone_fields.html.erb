<div class="form-group">
  <div class="control-label col-md-3">
    <%= f.label :number, 'Телефон' %>
  </div>
  <div class="col-md-6">
    <%= f.text_field :number, class: 'form-control', autofocus: true %>
  </div>    
</div>

<div class="form-group">
  <div class="control-label col-md-3">
    <%= f.label :extension, 'Добавочный' %>
  </div>
  <div class="col-md-6">
    <%= f.text_field :extension, class: 'form-control', autofocus: true %>
  </div>    
</div>

<div class="form-group">
  <div class="control-label col-md-3">
    <%= f.label :details, 'Примечания' %>
  </div>
  <div class="col-md-6">
    <%= f.text_area :details, class: 'form-control', autofocus: true %>
  </div>    
</div>

<div class="form-group">
  <div class="col-md-6 col-md-offset-3">
    <%= f.check_box :_destroy %>
    <%= f.label :_destroy, 'Удалить телефон' %>
  </div>
</div>